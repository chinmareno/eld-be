{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "python manage.py migrate --noinput && python manage.py bootstrap_superuser",
  "builds": [
    {
      "src": "project/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    }
  ]
}
